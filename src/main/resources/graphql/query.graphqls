scalar Date

type Query {
    country(code: String!): Country
    countries: [Country!]
}

type Mutation {
    add(input: CountryInput!): Country!
    update(input: CountryInput!): Country
}


type Country {
    id: ID!
    name: String!
    code: String!
    lastModifyDate: Date!
}

input CountryInput{
    name: String!
    code: String!
}